        "type": "object",
        "properties": {
          "categoryId": {
            "type": "integer",
            "format": "int32"
          },
          "categoryName": {
            "minLength": 1,
            "type": "string"
          },
          "restaurantId": {
            "type": "integer",
            "format": "int32"
