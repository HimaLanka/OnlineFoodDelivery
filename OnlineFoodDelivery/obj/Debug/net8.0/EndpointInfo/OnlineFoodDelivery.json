          },
          "itemPrice": {
            "type": "number",
            "format": "double"
          },
          "itemDescription": {
            "type": "string",
            "nullable": true
          },
          "isAvailable": {
            "type": "boolean"
          },
          "isVeg": {
            "type": "boolean"
          },
          "categoryId": {
            "type": "integer",
            "format": "int32"
          },
          "category": {
            "$ref": "#/components/schemas/MenuCategory"
          }
        },
        "additionalProperties": false
      },
      "Order": {
        "required": [
          "totalAmount",
          "userId"
        ],
        "type": "object",
        "properties": {
          "orderId": {
            "type": "integer",
            "format": "int32"
          },
          "totalAmount": {
            "type": "number",
            "format": "double"
          },
          "orderDate": {
            "type": "string",
            "format": "date-time"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "restaurantId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "restaurant": {
            "$ref": "#/components/schemas/Restaurant"
          },
          "cartItems": {
            "type": "array",
